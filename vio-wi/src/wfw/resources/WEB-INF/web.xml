<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>CORSandJSONPFilter</filter-name>
        <filter-class>vio.service.rest.filters.CORSandJSONPFilter</filter-class>
        <init-param>
            <param-name>JSONPParameter</param-name>
            <param-value>jsonp</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>CORSandJSONPFilter</filter-name>
        <url-pattern>/rst/*</url-pattern>
        <dispatcher>REQUEST</dispatcher>
    </filter-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
